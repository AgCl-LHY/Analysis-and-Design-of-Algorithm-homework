#include<iostream>
#include<vector>
#include<cmath>
#include<time.h>
#include<cstring>
#include<algorithm>
 
using namespace std;

int n,m;
int a=1,b=1;
int cnt=0;
int graph[20][20]={0};
int color[20]={0};

bool check(int c){
    for(int k=1;k<=n;k++){
        if(graph[c][k]&&color[c]==color[k]){
            return false;
         }
    }
    return true;
}

void dfs(int cur){
    if(cur>n){
        for(int i=1;i<=n;i++){
            printf("%d ",color[i]);
        }
        cnt++;
        printf("\n");
    }
    else{
        for(int i=1;i<=m;i++){
            color[cur]=i;
            if(check(cur)){
                dfs(cur+1);
            }
            color[cur]=0;
        }
    }
}

int main()
{
    scanf("%d %d",&n,&m);
    while(scanf("%d %d",&a,&b)!=EOF&&a!=0&&b!=0){
        graph[a][b]=1;
        graph[b][a]=1;
    }
    dfs(1);
    if(cnt > 0)
    {
        printf("Total=%d",cnt);
    }
    else
    {
        printf("NO\n");
    }
    system("pause");
    return 0;
}
